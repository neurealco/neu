{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["src/lib/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}